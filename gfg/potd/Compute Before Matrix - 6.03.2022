            for(int i=0; i<N;i++){
                for(int j = 0; j<M;j++){
                    out.print(before[i][j]);
                    out.print(' ');
                    
                }
                out.println();
            }
        }
        out.flush();
    }
}// } Driver Code Ends


//User function Template for Java

class Solution{
    public int[][] computeBeforeMatrix(int n, int m,int[][] arr ){
        // Code here
        int sum = 0;
        for(int i = 0; i<n;i++)
        {
            for(int j = m-1; j>0;j--)
            {
                arr[i][j] -= arr[i][j-1];
            }
        }
        for(int i = 0; i<m;i++)
        {
            for(int j = n-1; j>0;j--)
            {
                arr[j][i] -= arr[j-1][i];
            }
        }
        return arr;
    }
}
